{
  "name": "f1sh-analytics",
  "version": "1.2.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:server": "tsc --project tsconfig.server.json",
    "build:web": "next build",
    "build": "run-p build:*",
    "dev": "ts-node-dev --respawn -P tsconfig.server.json server/index.ts",
    "dev:server": "nodemon -e ts -w ./server -x yarn run dev:server:node",
    "dev:server:node": "ts-node --typeCheck -P tsconfig.server.json server/index.ts",
    "start:server": "cross-env NODE_ENV=production node dist/index.js",
    "start:migrate": "prisma migrate deploy && yarn run start"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "next": "^13.2.4",
    "node-fetch": "2",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.15.3",
    "@types/node-fetch": "2",
    "@types/react": "18.0.28",
    "autoprefixer": "^10.4.14",
    "nodemon": "^2.0.21",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "ts-node-dev": "^2.0.0"
  }
}